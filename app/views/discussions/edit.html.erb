<h1>Edit Discussion</h1>
<%= @discussion.errors.full_messages.join(", ") %>
<%= form_for [@project, @discussion] do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_area :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
    <%= f.submit %>
<% end %>
